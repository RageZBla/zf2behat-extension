<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
      <parameters>
        <parameter key="behat.zf2_extension.module">null</parameter>
        <parameter key="behat.zf2_extension.application.bootstrap">./init_autoloader.php</parameter>
        <parameter key="behat.zf2_extension.config.path">config/</parameter>
        <parameter key="behat.zf2_extension.config.file">application.config.php</parameter>

        
        <parameter key="behat.zf2_extension.context.initializer.class">Behat\Zf2Extension\Context\Initializer\Zf2AwareInizializer</parameter>
        <parameter key="behat.zf2_extension.zf2_config_path"></parameter>
        <parameter key="behat.zf2_extension.context.class_suffix">Features\Context\FeatureContext</parameter>
    </parameters>

    <services>
        
        <service id=""></service>
        
        <service id="behat.zf2_extension.context.initializer" class="%behat.zf2_extension.context.initializer.class%">
           <argument>%behat.zf2_extension.zf2_config_path%</argument>
           <tag name="behat.context.initializer" />
        </service>
        
        <service id="zf2.context.class_guesser" class="Behat\Zf2Extension\Context\ClassGuesser\Zf2ClassGuesser">
            <argument>%behat.zf2_extension.context.class_suffix%</argument>
            <tag name="behat.context.class_guesser" priority="5" />
        </service>
    </services>
</container>
